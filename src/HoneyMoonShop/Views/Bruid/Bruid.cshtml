@{
    Layout = "_HoneyMoon";
    ViewData["Title"] = "Home Page";
}
<!doctype html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link type="text/css" href="css/carrotcell.css" rel="stylesheet">
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script src="jquery-touchwipe.js" type="text/javascript" charset="utf-8"></script>


</head>
<body>
    <div class="container">
        <div class="row">
            <img src="~/Images/Bruidp1.png" alt="Bruid op zoek naar perfecte jurk, van begin tot eind kunnen we je helpen" />
        </div>

        <div class="row">
            <div class="col-md-8" style="height:640px">

                <div id="imageCarousel" class="carousel slide" data-interval="2000"
                     data-ride="carousel" data-pause="hover" data-wrap="true">

                    <ol class="carousel-indicators">
                        <li data-target="#imageCarousel" data-slide-to="0" class="active"></li>
                        <li data-target="#imageCarousel" data-slide-to="1"></li>
                        <li data-target="#imageCarousel" data-slide-to="2"></li>
                    </ol>

                    <div class="carousel-inner">
                        <div class="item active">
                            <div class="row">
                                <div class="col-md-2 style="height: 640px;">
                                    <img src="~/Images/BruidS1.png" class="img-responsive"> @*@from Afbeelding select J.SourcePath where J.artikelnummer = a.artikelnummer and j.NieuweCollectie= true *@
                                </div>

                                <div class="col-md-2 style="height: 640px;">
                                    <img src="~/Images/BruidS1.png" class="img-responsive"> @*@from Afbeelding select J.SourcePath where J.artikelnummer = a.artikelnummer and j.NieuweCollectie= true *@

                                </div>

                                <div class="col-md-2 style="height: 640px;">
                                    <img src="~/Images/BruidS3.png" class="img-responsive"> @*@from Afbeelding select J.SourcePath where J.artikelnummer = a.artikelnummer and j.NieuweCollectie= true *@

                                </div>

                                <div class="col-md-2 style="height: 640px;">
                                    <img src="~/Images/BruidS2.png" class="img-responsive"> @*@from Afbeelding select J.SourcePath where J.artikelnummer = a.artikelnummer and j.NieuweCollectie= true *@

                                </div>
                                <div class="col-md-2 style="height: 640px;">
                                    <img src="~/Images/BruidS1.png" class="img-responsive"> @*@from Afbeelding select J.SourcePath where J.artikelnummer = a.artikelnummer and j.NieuweCollectie= true *@

                                </div>

                            </div>
                        </div>



                    </div>

                    <a href="#imageCarousel" class="carousel-control left" data-slide="prev">
                        <span class="glyphicon glyphicon-chevron-left"></span>
                    </a>
                    <a href="#imageCarousel" class="carousel-control right" data-slide="next">
                        <span class="glyphicon glyphicon-chevron-right"></span>
                    </a>
                </div>

            </div>
        </div>
    </div>


    <div class="row">

    </div>
    </div>
</body>